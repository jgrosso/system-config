{
    "auto-close-tag.activationOnLanguage": [
        "html",
        "jsx",
        "php",
        "xml"
    ],

    "css.lint.boxModel": "warning",
    "css.lint.compatibleVendorPrefixes": "warning",
    "css.lint.duplicateProperties": "error",
    "css.lint.emptyRules": "error",
    "css.lint.ieHack": "warning",
    "css.lint.importStatement": "warning",
    "css.lint.propertyIgnoredDueToDisplay": "error",
    "css.lint.unknownVendorSpecificProperties": "warning",
    "css.lint.vendorPrefix": "warning",
    "css.lint.zeroUnits": "error",

    "debug.allowBreakpointsEverywhere": true,
    "debug.inlineValues": true,

    "diffEditor.ignoreTrimWhitespace": false,

    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.cursorBlinking": "smooth",
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "'Input Mono', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.minimap.enabled": true,
    "editor.parameterHints": false,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        120
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.smoothScrolling": true,
    "editor.tabSize": 2,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "editor.wrappingIndent": "none",

    "explorer.autoReveal": false,
    "explorer.openEditors.visible": 0,

    "extensions.autoUpdate": true,

    "files.associations": {
        "*.m": "matlab"
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/bower_components/**": true,
        "**/node_modules/**": true,
        "**/out/**": true
    },

    "flow.enabled": false,

    "git.confirmSync": true,

    "html.format.extraLiners": "",
    "html.format.indentInnerHtml": true,

    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "javascript.validate.enable": false,

    "less.lint.boxModel": "warning",
    "less.lint.compatibleVendorPrefixes": "warning",
    "less.lint.duplicateProperties": "error",
    "less.lint.emptyRules": "error",
    "less.lint.ieHack": "warning",
    "less.lint.importStatement": "warning",
    "less.lint.propertyIgnoredDueToDisplay": "error",
    "less.lint.unknownVendorSpecificProperties": "warning",
    "less.lint.vendorPrefix": "warning",
    "less.lint.zeroUnits": "error",

    "markdown.preview.markEditorSelection": false,

    "search.exclude": {
        "**/bower_components": true,
        "**/node_modules": true,
        "**/out": true
    },
    "search.useIgnoreFiles": true,

    "scss.lint.boxModel": "warning",
    "scss.lint.compatibleVendorPrefixes": "warning",
    "scss.lint.duplicateProperties": "error",
    "scss.lint.emptyRules": "error",
    "scss.lint.ieHack": "warning",
    "scss.lint.importStatement": "warning",
    "scss.lint.propertyIgnoredDueToDisplay": "error",
    "scss.lint.unknownVendorSpecificProperties": "warning",
    "scss.lint.vendorPrefix": "warning",
    "scss.lint.zeroUnits": "error",

    "telemetry.enableTelemetry": false,

    "terminal.integrated.macOptionIsMeta": true,
    "terminal.external.osxExec": "iTerm2.app",

    "typescript.check.npmIsInstalled": false,
    "typescript.useCodeSnippetsOnMethodSuggest": true,

    "vim.easymotion": true, // https://raw.githubusercontent.com/VSpaceCode/VSpaceCode/master/settings.json
